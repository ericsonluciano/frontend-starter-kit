{
  "name": "frontend-starter-kit",
  "version": "1.0.0",
  "author": "Rico Sta. Cruz <rico@ricostacruz.com> (http://ricostacruz.com)",
  "babel": {
    "presets": [
      "es2017"
    ],
    "plugins": [
      "syntax-jsx",
      [
        "transform-react-jsx",
        {
          "pragma": "element"
        }
      ]
    ]
  },
  "dependencies": {
    "babel-plugin-syntax-jsx": "6.13.0",
    "babel-plugin-transform-react-jsx": "6.8.0",
    "babel-preset-es2017": "6.14.0",
    "babelify": "7.3.0",
    "browserify": "13.1.0",
    "jstransformer-pug": "0.2.1",
    "metalsmith": "2.2.0",
    "metalsmith-browserify": "0.1.0",
    "metalsmith-jstransformer": "0.7.1",
    "metalsmith-sense-sass": "1.1.0",
    "metalsmith-uglifyjs": "github:rstacruz/metalsmith-uglifyjs#patch-1"
  },
  "devDependencies": {
    "metalsmith-start": "2.0.1",
    "watchify": "3.7.0"
  },
  "keywords": [],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "env NODE_ENV=production node metalsmith.js",
    "prepublish": "npm run build",
    "start": "metalsmith-start",
    "test": "echo \"Error: no test specified\" && exit 1"
  }
}
